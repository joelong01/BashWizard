{
    "name": "bashwizard",
    "productName": "BashWizard",
    "version": "1.0.0",
    "private": true,
    "main": "./build/main.js",
    "homepage": ".",
    "dependencies": {
        "@babel/runtime": "^7.4.5",
        "@fortawesome/fontawesome-free": "^5.8.2",
        "@types/lodash-es": "^4.17.3",
        "@types/shortid": "0.0.29",
        "bash-models": "^1.1.3",
        "classnames": "^2.2.6",
        "electron-titlebar-windows": "^3.0.0",
        "electron-window-state": "^5.0.3",
        "fs": "0.0.1-security",
        "jquery": "^3.4.1",
        "lodash-es": "^4.17.11",
        "njv": "^2.0.0",
        "popper.js": "^1.15.0",
        "primeflex": "^1.0.0-rc.1",
        "primeicons": "^1.0.0",
        "primereact": "^3.1.3",
        "rc-menu": "^7.4.22",
        "react": "^16.8.4",
        "react-ace": "^6.5.0",
        "react-dom": "^16.8.4",
        "react-split-pane": "^0.1.87",
        "react-splitter-layout": "^3.0.1",
        "react-svg": "^7.2.14",
        "react-transition-group": "^2.6.1",
        "shortid": "^2.2.14",
        "strongly-typed-events": "^1.4.10",
        "universal-cookie": "^3.1.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "scripts": {
        "clean": "rimraf ./dist && rimraf ./build && rimraf ./releases",
        "start": "nf start -p 3000",
        "compile": "tsc",
        "build": "npm run clean && react-scripts build",
        "webpack:dev": "webpack --config ./config/webpack.dev.js",
        "webpack:prod": "webpack -p --config ./config/webpack.prod.js",
        "react-start": "npm run clean && react-scripts start",
        "electron:run:dev": "npm run webpack:dev && electron . --remote-debugging-port=9223",
        "electron:run:prod": "npm run webpack:prod && electron . --remote-debugging-port=9223",
        "electron:start:dev": "npm run webpack:dev && npm run electron-start",
        "electron:start:prod": "npm run webpack:prod && npm run electron-start",
        "electron-start": "node src/electron/start",
        "eject": "react-scripts eject",
        "release": "npm run build && npm run webpack:prod && electron-builder",
        "pretest": "./node_modules/.bin/tslint 'src/**/*.ts*'",
        "lintfix": "./node_modules/.bin/tslint 'src/**/*.ts*' --fix",
        "test": "react-scripts test --env=jsdom --silent",
        "test:ci": "cross-env CI=true npm run test",
        "test:coverage": "npm run test -- --coverage",
        "plato": "scripts/generate-report.sh -o report -v $(node -pe \"require('./package.json').version\") -c $(git rev-parse --short HEAD)",
        "postinstall": "electron-builder install-app-deps",
        "predebug": "npm run build && npm run webpack:dev"
    },
    "devDependencies": {
        "@types/jest": "^23.3.14",
        "@types/node": "^10.14.7",
        "@types/react": "^16.8.18",
        "@types/react-dom": "^16.8.4",
        "electron": "^4.2.2",
        "electron-builder": "^20.40.2",
        "electron-packager": "^13.1.1",
        "foreman": "^3.0.1",
        "node-sass": "^4.12.0",
        "react-scripts": "^2.1.8",
        "rimraf": "^2.6.3",
        "ts-loader": "^5.4.5",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.2",
        "webpack-merge": "^4.1.5"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
